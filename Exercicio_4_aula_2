package plp_aula_java2;

/**
 *
 * @author Acer
 */
public class Exercicio_4_Aula_2 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        for(int i=0; i<8; i++){
            System.out.println(i);
            if (i==5){
                break;
            }
        }
    }
    
}
